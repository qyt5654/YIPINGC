#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/F:/留言墙/server/node_modules/.store/ejs@3.1.10/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/F:/留言墙/server/node_modules/.store/ejs@3.1.10/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.store/ejs@3.1.10/node_modules/ejs/bin/cli.js" "$@"
else
  exec node  "$basedir/../.store/ejs@3.1.10/node_modules/ejs/bin/cli.js" "$@"
fi
